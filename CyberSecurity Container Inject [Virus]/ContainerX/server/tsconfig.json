{
    "compilerOptions":
    {
        "target": "ES2020",
        "module": "commonjs",
        "lib":
        [
            "DOM",
            "DOM.Iterable",
            "ES2020",
            "ESNext",
            "ScriptHost"
        ],
    
        "outDir": "./dist/",
        "rootDir": "./src/",
        "removeComments": true,
    
        "strict": true,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "noImplicitReturns": true,
        "noEmitOnError": true,
    
        "esModuleInterop": true,
    }
}
